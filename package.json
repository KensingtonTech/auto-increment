    "mongoose": "~5.9.25",
    "@types/mongoose": "~5.7.32"
  },
  "devDependencies": {
    "@types/mongodb": "~3.6.x",
    "@typegoose/typegoose": "~7.3.5",
    "@types/jest": "^26.0.14",
    "@types/mongoose": "~5.7.36",
    "@types/node": "^10.17.16",
    "@types/semver": "^7.3.4",
    "coveralls": "^3.1.0",
    "jest": "^26.5.3",
    "mongodb-memory-server": "~6.9.2",
